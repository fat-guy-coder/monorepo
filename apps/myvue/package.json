{
  "name": "myvue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "translate": "tsx src/language/generate.ts",
    "g": "node  --import=tsx  scripts/generateRouteAndView.ts",
    "rg": "node  --import=tsx  scripts/rg.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@ld/micro-frontend": "workspace:*",
    "@vue/compiler-sfc": "^3.5.25",
    "codemirror": "^6.0.2",
    "less": "^4.4.2",
    "magic-string": "^0.30.21",
    "pinia": "^3.0.1",
    "prismjs": "^1.29.0",
    "utils": "workspace:^",
    "vue": "3.6.0-alpha.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue": "beta",
    "@vitejs/plugin-vue-jsx": "beta",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-oxlint": "^0.15.10",
    "eslint-plugin-vue": "^9.32.0",
    "oxlint": "^0.15.10",
    "unplugin-vue-components": "^28.4.0",
    "vite-plugin-vue-devtools": "latest",
    "vue-tsc": "^2.2.2"
  }
}
