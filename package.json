{
  "name": "json",
  "version": "1.0.0",
  "description": "我的项目啊",
  "main": "main.js",
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "devDependencies": {
    "@inquirer/prompts": "^8.0.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.4",
    "@vitest/eslint-plugin": "1.1.31",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "ora": "^8.2.0",
    "prettier": "^3.5.1",
    "start-server-and-test": "^2.0.10",
    "terser": "^5.44.1",
    "tsx": "^4.19.3",
    "turbo": "^2.5.5",
    "vite": "^7.2.4",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.0.5"
  },
  "bin": {
    "gen": "./bin/cli.js"
  },
  "scripts": {
    "//-- Individual Dev Servers --//": "",
    "main:dev": "pnpm --filter @ld/main run dev",
    "myvue:dev": "pnpm --filter myvue run dev",
    "//-- Turbo Build Commands --//": "",
    "turbo:all:build": "turbo run build",
    "turbo:myvue:build": "turbo run build --filter=myvue",
    "turbo:main:build": "turbo run build --filter=@ld/main",
    "turbo:micro-frontend:build": "turbo run build --filter=@ld/micro-frontend",
    "turbo:project:build": "turbo run build --filter=\"$1\"",
    "//-- Production Build --//": "",
    "main:build": "pnpm --filter @ld/main run build",
    "myvue:build": "pnpm --filter myvue run build",
    "mf:build": "pnpm --filter @ld/micro-frontend run build",
    "all:build": "run-p \"build:**\"",
    "//--  Preview --//": "",
    "preview:myvue": "pnpm --filter myvue preview --port 8080",
    "preview:main": "pnpm --filter @ld/main preview --port 9090",
    "prod:preview": "run-p \"preview:**\"",
    "//-- Other --//": "",
    "g": "pnpm -F myvue g",
    "translate": "pnpm -F myvue translate",
    "test": "test"
  },
  "keywords": [
    "前端学习网站",
    "视效介绍网站"
  ],
  "author": "嗨嗨嗨",
  "license": "ISC",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "@tiptap/core": "2.27.1",
      "@tiptap/pm": "2.27.1"
    }
  }
}