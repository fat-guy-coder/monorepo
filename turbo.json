{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build:front-learning": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": []
    },
    "build:main": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build:micro-frontend": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": [],
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "outputs": []
    }
  },
  "globalDependencies": [
    ".env"
  ],
  "globalEnv": [
    "API_KEY",
    "ENV"
  ]
}